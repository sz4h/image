{
  "name": "space/image",
  "description": "This is my package image",
  "keywords": [
    "space",
    "laravel",
    "image"
  ],
  "homepage": "https://github.com/space/image",
  "license": "MIT",
  "authors": [
    {
      "name": "Ahmed Safaa",
      "email": "ahmed@sz4h.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1",
    "intervention/image": "^2",
    "spatie/laravel-package-tools": "^1.9.2",
    "illuminate/contracts": "^9.0"
  },
  "require-dev": {
    "laravel/pint": "^1.0",
    "nunomaduro/collision": "^6.0",
    "nunomaduro/larastan": "^2.0.1",
    "orchestra/testbench": "^7.0",
    "pestphp/pest": "^1.21",
    "pestphp/pest-plugin-laravel": "^1.1",
    "phpstan/extension-installer": "^1.1",
    "phpstan/phpstan-deprecation-rules": "^1.0",
    "phpstan/phpstan-phpunit": "^1.0",
    "phpunit/phpunit": "^9.5",
    "spatie/laravel-ray": "^1.26"
  },
  "autoload": {
    "files": [
      "helpers/helpers.php"
    ],
    "psr-4": {
      "Space\\Image\\": "src",
      "Space\\Image\\Database\\Factories\\": "database/factories"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Space\\Image\\Tests\\": "tests"
    }
  },
  "scripts": {
    "analyse": "vendor/bin/phpstan analyse",
    "test": "vendor/bin/pest",
    "test-coverage": "vendor/bin/pest --coverage",
    "format": "vendor/bin/pint"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "phpstan/extension-installer": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Space\\Image\\ImageServiceProvider"
      ],
      "aliases": {
        "Image": "Space\\Image\\Facades\\Image"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
